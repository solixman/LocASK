generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Answer {
  id          String   @id @default(uuid())
  content     String
  userId      String   // was Int
  questionId  String
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}

